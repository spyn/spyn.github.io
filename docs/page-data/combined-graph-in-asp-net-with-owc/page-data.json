Add</span><span class=\"token punctuation\">(</span><span class=\"token number\">0</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span>\n    seriesValue<span class=\"token punctuation\">.</span>Caption <span class=\"token operator\">=</span> <span class=\"token string\">\"Value Array\"</span><span class=\"token punctuation\">;</span>\n    seriesValue<span class=\"token punctuation\">.</span><span class=\"token function\">SetData</span><span class=\"token punctuation\">(</span> ChartDimensionsEnum<span class=\"token punctuation\">.</span>chDimCategories<span class=\"token punctuation\">,</span>\n    Convert<span class=\"token punctuation\">.</span><span class=\"token function\">ToInt32</span><span class=\"token punctuation\">(</span>ChartSpecialDataSourcesEnum<span class=\"token punctuation\">.</span>chDataLiteral<span class=\"token punctuation\">)</span><span class=\"token punctuation\">,</span> dateArray<span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span>\n    seriesValue<span class=\"token punctuation\">.</span><span class=\"token function\">SetData</span><span class=\"token punctuation\">(</span> ChartDimensionsEnum<span class=\"token punctuation\">.</span>chDimValues<span class=\"token punctuation\">,</span>\n    Convert<span class=\"token punctuation\">.</span><span class=\"token function\">ToInt32</span><span class=\"token punctuation\">(</span>ChartSpecialDataSourcesEnum<span class=\"token punctuation\">.</span>chDataLiteral<span class=\"token punctuation\">)</span><span class=\"token punctuation\">,</span> valueArray<span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span>\n    seriesValue<span class=\"token punctuation\">.</span>Interior<span class=\"token punctuation\">.</span>Color <span class=\"token operator\">=</span> <span class=\"token string\">\"#FF9E00\"</span><span class=\"token punctuation\">;</span>\n\n    seriesMoreValue <span class=\"token operator